#include <bits/stdc++.h>
using namespace std;

int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(0);
#ifndef ONLINE_JUDGE
    freopen("in.txt", "r", stdin);
    freopen("out.txt", "w", stdout);
    freopen("error.txt","w",stderr);
#endif

    int m , n ,k ;
    cin>>m>>n>>k;
    char a[m][n] ;
    for (int i = 1; i <=m; ++i)
    {
        for (int j = 1; j <=n; ++j)
        {
            cin>>a[i][j];
        }
    }

    for (int i = 1; i <=m; ++i)
    {
        for(int z = 1 ; z<=k;++z) {
            for (int j = 1; j <= n; ++j) {
                for (int l = 1; l <= k; ++l) {
                    cout << a[i][j];
                }
            }
            cout << endl;
        }
    }


}

